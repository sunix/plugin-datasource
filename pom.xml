                <artifactId>codenvy-ext-tutorials</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-web</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-api</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-core</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-env-dummy</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-security</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.google.gwt.inject</groupId>
                <artifactId>gin</artifactId>
                <version>${gin.version}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.commons</groupId>
                <artifactId>commons-lang3</artifactId>
                <version>${commons.lang.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nuodb.jdbc</groupId>
                <artifactId>nuodb-jdbc</artifactId>
                <version>${nuodb-driver.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>net.sf.opencsv</groupId>
                <artifactId>opencsv</artifactId>
                <version>${opencsv.version}</version>
            </dependency>
            <dependency>
                <groupId>net.sourceforge.schemacrawler</groupId>
                <artifactId>schemacrawler</artifactId>
                <version>${schemacrawler.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-client</artifactId>
                <version>${codenvy.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>com.codenvy.platform-api</groupId>
                <artifactId>codenvy-api-project</artifactId>
                <version>${com.codenvy.platform-api.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>com.codenvy.platform-api</groupId>
                <artifactId>codenvy-api-vfs</artifactId>
                <version>${com.codenvy.platform-api.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>com.codenvy.sdk</groupId>
                <artifactId>assembly-platform-api</artifactId>
