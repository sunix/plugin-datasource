<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>codenvy-ide-parent</artifactId>
        <groupId>com.codenvy.ide</groupId>
        <version>3.0.0-M14-SNAPSHOT</version>
    </parent>
    <artifactId>codenvy-ext-datasource-parent</artifactId>
    <version>3.0.0-M14-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>Codenvy Extension :: Datasource parent</name>
    <modules>
        <module>codenvy-ext-datasource-shared</module>
        <module>codenvy-ext-datasource-server</module>
        <module>codenvy-ext-datasource-client</module>
        <module>codenvy-ext-datasource-api-war</module>
        <module>codenvy-ext-datasource-application-war</module>
        <module>codenvy-ext-datasource-packaging-standalone-tomcat</module>
        <module>codenvy-ext-datasource-qa</module>
    </modules>
    <distributionManagement>
        <repository>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/repositories/codenvy-private-releases/</url>
        </repository>
        <snapshotRepository>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/repositories/codenvy-private-snapshots/</url>
        </snapshotRepository>
        <site>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/groups/codenvy-public-group/</url>
        </site>
    </distributionManagement>
    <properties>
        <codenvy.datasource.version>3.0.0-M14-SNAPSHOT</codenvy.datasource.version>
        <codenvy.version>3.0.0-M14-SNAPSHOT</codenvy.version>
        <commons.lang.version>3.2.1</commons.lang.version>
        <dto-generator-out-directory>${project.build.directory}/generated-sources/dto/</dto-generator-out-directory>
        <jtds-driver.version>1.2</jtds-driver.version>
        <mysql-driver.version>5.1.29</mysql-driver.version>
        <nuodb-driver.version>2.0.3</nuodb-driver.version>
        <opencsv.version>2.3</opencsv.version>
        <oracle-driver.version>11.2.0</oracle-driver.version>
        <postgresql-driver.version>9.3-1100-jdbc41</postgresql-driver.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <schemacrawler.version>10.08.01</schemacrawler.version>
        <sqlserver-driver.version>4.0</sqlserver-driver.version>
    </properties>
    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.codenvy.commons</groupId>
                <artifactId>codenvy-commons-servlet</artifactId>
                <version>${com.codenvy.commons.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-builder</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-datasource-client</artifactId>
                <version>${codenvy.datasource.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-datasource-server</artifactId>
                <version>${codenvy.datasource.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-datasource-shared</artifactId>
                <version>${codenvy.datasource.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-datasource-packaging-standalone-tomcat</artifactId>
                <version>${codenvy.datasource.version}</version>
                <type>zip</type>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-debugger-java</artifactId>
                <version>${codenvy.version}</version>
                <exclusions>
                    <exclusion>
                        <artifactId>tools</artifactId>
                        <groupId>sun.jdt</groupId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-git</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-github</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
